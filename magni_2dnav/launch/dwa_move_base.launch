<launch>

   <master auto="start"/>
 <!-- Run the map server --> 
    <node name="map_server" pkg="map_server" type="map_server" args="$(find magni_2dnav)/maps/better_map.yaml"/>

 <!--- Run AMCL --> 
    <include file="$(find magni_2dnav)/launch/amcl_magni.launch" />

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    <rosparam file="$(find magni_2dnav)/param/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find magni_2dnav)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find magni_2dnav)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find magni_2dnav)/param/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find magni_2dnav)/param/dwa_local_planner_params.yaml" command="load" />
 </node>

</launch> 
